  "pipelines": {
    "syllabus_to_calendar": {
      "name": "Syllabus to Calendar Pipeline",
      "description": "Parse syllabus, extract events, create calendar entries",
      "stages": [
        {
          "name": "parse",
          "handler": "parsers/syllabus",
          "input": "pdf/docx",
          "output": "events_json"
        },
        {
          "name": "normalize",
          "handler": "scheduling/schedule_optimizer",
          "input": "events_json",
          "output": "normalized_events"
        },
        {
          "name": "calendar_sync",
          "handler": "integrations/calendars",
          "input": "normalized_events",
          "output": "calendar_entries"
        }
      ],
      "triggers": [
        "manual",
        "file_upload"
      ]
    },
    "media_processing": {
      "name": "Media Processing Pipeline",
      "description": "Convert, compress, extract subtitles from media",
      "stages": [
        {
          "name": "analyze",
          "handler": "tools/ffmpeg",
          "input": "media_file",
          "output": "media_info"
        },
        {
          "name": "convert",
          "handler": "convertx",
          "input": "media_file",
          "output": "converted_file"
        },
        {
          "name": "extract_subs",
          "handler": "tools/ffmpeg",
          "input": "media_file",
          "output": "subtitle_file"
        },
        {
          "name": "transcribe",
          "handler": "agents/live_caption",
          "input": "audio",
          "output": "transcript_md"
        }
      ],
      "triggers": [
        "file_upload",
        "manual"
      ]
    },
    "research_workflow": {
      "name": "Research Workflow Pipeline",
      "description": "Ingest documents, create embeddings, enable RAG",
      "stages": [
        {
          "name": "ingest",
          "handler": "workspace/incoming",
          "input": "documents",
          "output": "staged_docs"
        },
        {
          "name": "chunk",
          "handler": "integrations/rag_pipeline",
          "input": "staged_docs",
          "output": "chunks"
        },
        {
          "name": "embed",
          "handler": "chromadb",
          "input": "chunks",
          "output": "embeddings"
        },
        {
          "name": "index",
          "handler": "librarian",
          "input": "embeddings",
          "output": "searchable_index"
        }
      ],
      "triggers": [
        "file_upload",
        "scheduled"
      ]
    },
    "daily_brief_workflow": {
      "name": "Daily Brief Pipeline",
      "description": "Aggregate data and generate morning brief",
      "stages": [
        {
          "name": "collect_calendar",
          "handler": "integrations/calendars",
          "input": "date_range",
          "output": "events"
        },
        {
          "name": "collect_tasks",
          "handler": "agents/personal_assistant",
          "input": "filters",
          "output": "tasks"
        },
        {
          "name": "generate_brief",
          "handler": "agents/daily_brief",
          "input": "aggregated_data",
          "output": "brief_md"
        }
      ],
      "triggers": [
        "scheduled:daily:0700"
      ]
    },
    "note_integration": {
      "name": "Note Integration Pipeline",
      "description": "Process class notes, link to references, integrate into knowledge",
      "stages": [
        {
          "name": "analyze",
          "handler": "agents/research_intel",
          "input": "raw_notes",
          "output": "analyzed_notes"
        },
        {
          "name": "link_refs",
          "handler": "agents/librarian",
          "input": "analyzed_notes",
          "output": "linked_notes"
        },
        {
          "name": "embed",
          "handler": "chromadb",
          "input": "linked_notes",
          "output": "note_embeddings"
        },
        {
          "name": "integrate",
          "handler": "agents/knowledge_management",
          "input": "linked_notes",
          "output": "obsidian_note"
        }
      ],
      "triggers": [
        "manual",
        "file_upload"
      ],
      "requires_approval": true
    },
