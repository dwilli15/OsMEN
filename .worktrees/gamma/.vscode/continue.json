{
  "models": [
    {
      "title": "GPT-4 (Primary)",
      "provider": "openai",
      "model": "gpt-4",
      "apiKey": "${OPENAI_API_KEY}",
      "contextLength": 8192
    },
    {
      "title": "Claude 3 Opus",
      "provider": "anthropic",
      "model": "claude-3-opus-20240229",
      "apiKey": "${ANTHROPIC_API_KEY}",
      "contextLength": 200000
    },
    {
      "title": "LM Studio (Local)",
      "provider": "openai",
      "model": "local-model",
      "apiBase": "http://localhost:1234/v1",
      "contextLength": 4096
    },
    {
      "title": "Ollama Mistral",
      "provider": "ollama",
      "model": "mistral",
      "apiBase": "http://localhost:11434",
      "contextLength": 8192
    }
  ],
  "tabAutocompleteModel": {
    "title": "LM Studio Autocomplete",
    "provider": "openai",
    "model": "local-model",
    "apiBase": "http://localhost:1234/v1"
  },
  "embeddingsProvider": {
    "provider": "ollama",
    "model": "nomic-embed-text",
    "apiBase": "http://localhost:11434"
  },
  "slashCommands": [
    {
      "name": "edit",
      "description": "Edit selected code"
    },
    {
      "name": "comment",
      "description": "Add comments to code"
    },
    {
      "name": "test",
      "description": "Generate unit tests"
    },
    {
      "name": "fix",
      "description": "Fix code issues"
    },
    {
      "name": "explain",
      "description": "Explain code"
    }
  ],
  "customCommands": [
    {
      "name": "osmen-agent",
      "prompt": "You are an OsMEN agent specialist. Help with agent implementation, Langflow flows, and n8n workflows.",
      "description": "OsMEN-specific assistance"
    }
  ],
  "contextProviders": [
    {
      "name": "diff",
      "params": {}
    },
    {
      "name": "open",
      "params": {}
    },
    {
      "name": "terminal",
      "params": {}
    },
    {
      "name": "problems",
      "params": {}
    },
    {
      "name": "code",
      "params": {}
    }
  ],
  "allowAnonymousTelemetry": false,
  "docs": [
    "https://github.com/dwilli15/OsMEN"
  ]
}
